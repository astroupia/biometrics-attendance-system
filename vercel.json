{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python",
      "maxDuration": 10
    },
    "index.js": {
      "runtime": "@vercel/node",
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
