{
  "version": 2,
  "builds": [
    {
      "src": "app/app.py",
      "use": "@vercel/python"
    },
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "build": {
    "env": {
      "PYTHONUNBUFFERED": "1"
    }
  }
}
