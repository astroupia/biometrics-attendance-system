{
  "version": 2,
  "builds": [
    {
      "src": "app/app.py",
      "use": "@vercel/python@3.9"
    },
    {
      "src": "app.js",
      "use": "@vercel/node@14"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "app/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "build": {
    "env": {
      "PYTHONUNBUFFERED": "1"
    }
  },
  "functions": {
    "api/*.py": {
      "runtime": "python@3.9"
    }
  }
}
